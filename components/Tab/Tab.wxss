.tab_container .tab_title {
  background-color: #ffffff;
  display: flex;
  flex-direction: row;
}
.tab_container .tab_title .title_item {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  padding: 12rpx 0;
  color: #a2a2a2;
}
.tab_container .tab_title .active {
  color: var(--themeColor);
  border-bottom: 6rpx solid currentColor;
}
